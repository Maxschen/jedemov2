{
  "title": "Examples Schema",
  "schema": {
    "title": "Json-Editor Interactive Playground Example format",
    "type": "object",
    "required": [
      "title",
      "desc",
      "schema",
      "startval",
      "code",
      "config"
    ],
    "properties": {
      "title": {
        "title": "Example Title",
        "type": "string",
        "options": {
          "inputAttributes": {
            "placeholder": "Enter  the name of the example"
          }
        }
      },
      "desc": {
        "title": "Example Description",
        "type": "string",
        "format": "html",
        "options": {
          "wysiwyg": true
        }
      },
      "schema": {
        "title": "Example Schema",
        "type": "string",
        "format": "json"
      },
      "startval": {
        "title": "Example Start Value",
        "type": "string",
        "format": "json"
      },
      "code": {
        "title": "Example JavaScript",
        "type": "string",
        "format": "javascript"
      },
      "config": {
        "title": "Example Config",
        "type": "string",
        "format": "json"
      }
    }
  },
  "startval": {},
  "config": {
    "theme": "bootstrap3",
    "iconlib": "bootstrap3",
    "object_layout": "normal",
    "template": "default",
    "show_errors": "interaction",
    "ajax": 1
  },
  "code": "",
  "desc": "Add optional description here. (HTML format)"
}